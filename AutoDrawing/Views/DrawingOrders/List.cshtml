@{
    ViewData["Title"] = "Order List";
}

<div id="Modal_ServiceList" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <span class="modal-title"></span>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">

            </div>
        </div>
    </div>
</div>

<div id="" class="container-fluid">
    <div class="row" id="ShipyardListPage"></div>
</div>


@section styles{
    <environment include="Development">
        <link rel="stylesheet" href="~/lib/toastr.js/toastr.css" />
        <link rel="stylesheet" href="~/css/DrawingOrderList.css" />
    </environment>

    <environment exclude="Development">
        <link rel="stylesheet" href="~/lib/toastr.js/toastr.min.css" />
        <link rel="stylesheet" href="~/css/DrawingOrderList.min.css" />
    </environment>
}

@section scripts{
    <script src="~/lib/toastr.js/toastr.min.js"></script>

    <environment include="Development">
        <script src="~/js/DrawingOrderList.js"></script>
    </environment>

    <environment exclude="Development">
        <script src="~/js/DrawingOrderList.min.js"></script>
    </environment>
}